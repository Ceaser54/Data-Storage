<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.stage2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/inventory"
        android:label="@string/app_name"
        android:roundIcon="@drawable/inventory"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.EditProductDetails" />
        <activity android:name=".main.SeeProductDetails" />
        <activity android:name=".main.AddProduct"></activity>
    </application>
</manifest>